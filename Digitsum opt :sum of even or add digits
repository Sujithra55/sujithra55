import java.io.*;
import java .util.*;
Class UserMainCode{
Public int evenOddDigitsSum(int input1,string input2){
if(input2.equals("Odd")){
int sum=0;

While(input1>0){

int r=input1%10;
if(r%2==1){

sum+=r;
}

input1=input1/10;

}
return sum;
}
else
{
int sum=0;
while(input1>0)
{
int r=input1%10;
if(r%2==0){

sum+=r;


}
input1/=10;
}
return sum;
}
}
}
